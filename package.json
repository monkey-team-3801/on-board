{
    "name": "on-board",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "npm run build-client && npm run build-server",
        "build-client": "node scripts/build.js",
        "build-server": "webpack --config ./config/webpackServer.config.js",
        "eslint": "eslint --fix --ext .tsx,ts,jsx,js src/",
        "eslint-check": "eslint --ext .tsx,ts,jsx,js src/ --max-warnings=0",
        "fetch-course": "node scripts/fetchCourse.js",
        "prettier": "prettier --write .",
        "prettier-check": "prettier --check .",
        "start": "npm run start-server",
        "start-client": "node scripts/start.js",
        "start-client-dev": "node scripts/start.js",
        "start-server": "node build/server.js",
        "start-server-dev": "ts-node-dev src/server/server.ts",
        "test": "node scripts/test.js"
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "**/*.{js,jsx,ts,tsx}",
            "!**/*.d.ts"
        ],
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "modulePaths": [],
        "roots": [
            "<rootDir>/"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.ts"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "testMatch": [
            "<rootDir>/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "dependencies": {
        "@react-hook/throttle": "^2.2.0",
        "@svgr/webpack": "4.3.3",
        "axios": "^0.19.2",
        "body-parser": "^1.19.0",
        "bootstrap": "^4.5.2",
        "camelcase": "^5.3.1",
        "date-fns": "^2.15.0",
        "dotenv": "8.2.0",
        "dotenv-expand": "5.1.0",
        "express": "^4.17.1",
        "express-fileupload": "^1.2.0",
        "file-loader": "4.3.0",
        "fs-extra": "^8.1.0",
        "identity-obj-proxy": "3.0.0",
        "immutable": "^4.0.0-rc.12",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.20",
        "mdbreact": "^4.27.0",
        "mongodb": "^3.6.0",
        "mongoose": "^5.10.0",
        "node-schedule": "^1.3.2",
        "peer": "^0.5.3",
        "peerjs": "^1.3.1",
        "qs": "^6.9.4",
        "react": "^16.13.1",
        "react-app-polyfill": "^1.0.6",
        "react-bootstrap": "^1.3.0",
        "react-color": "^2.18.1",
        "react-dom": "^16.13.1",
        "react-dropzone": "^11.0.3",
        "react-fade-in": "^1.1.1",
        "react-icons": "^3.11.0",
        "react-particles-js": "^3.3.0",
        "react-router-dom": "^5.2.0",
        "react-select": "^3.1.0",
        "react-socket-io": "^0.2.5",
        "recharts": "^1.8.5",
        "recordrtc": "^5.6.1",
        "resolve": "1.15.0",
        "resolve-url-loader": "3.1.1",
        "semver": "6.3.0",
        "sha3": "^2.1.3",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.0",
        "use-debounce": "^5.0.0",
        "uuid": "^8.3.0"
    },
    "devDependencies": {
        "@babel/core": "7.9.0",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/express": "^4.17.7",
        "@types/express-fileupload": "^1.1.4",
        "@types/jest": "^24.0.0",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lodash": "^4.14.160",
        "@types/mongodb": "^3.5.26",
        "@types/mongoose": "^5.7.36",
        "@types/node": "^12.0.0",
        "@types/node-schedule": "^1.3.0",
        "@types/react": "^16.9.0",
        "@types/react-bootstrap": "^0.32.22",
        "@types/react-color": "^3.0.4",
        "@types/react-dom": "^16.9.0",
        "@types/react-router-dom": "^5.1.5",
        "@types/react-select": "^3.0.19",
        "@types/recharts": "^1.8.16",
        "@types/recordrtc": "^5.6.2",
        "@types/socket.io": "^2.1.11",
        "@types/socket.io-client": "^1.4.33",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "babel-eslint": "10.1.0",
        "babel-jest": "^24.9.0",
        "babel-loader": "8.1.0",
        "babel-plugin-named-asset-import": "^0.3.6",
        "babel-preset-react-app": "^9.1.2",
        "case-sensitive-paths-webpack-plugin": "2.3.0",
        "css-loader": "3.4.2",
        "eslint": "^7.7.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-react-app": "^5.2.1",
        "eslint-loader": "3.0.3",
        "eslint-plugin-flowtype": "4.6.0",
        "eslint-plugin-import": "2.20.1",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-react": "7.19.0",
        "eslint-plugin-react-hooks": "^1.6.1",
        "html-webpack-plugin": "4.0.0-beta.11",
        "http-proxy-middleware": "^1.0.5",
        "jest": "24.9.0",
        "jest-environment-jsdom-fourteen": "1.0.1",
        "jest-resolve": "24.9.0",
        "jest-watch-typeahead": "0.4.2",
        "less-loader": "^6.2.0",
        "mini-css-extract-plugin": "0.9.0",
        "optimize-css-assets-webpack-plugin": "5.0.3",
        "pnp-webpack-plugin": "1.6.4",
        "postcss-flexbugs-fixes": "4.1.0",
        "postcss-loader": "3.0.0",
        "postcss-normalize": "8.0.1",
        "postcss-preset-env": "6.7.0",
        "postcss-safe-parser": "4.0.1",
        "prettier": "^2.1.1",
        "react-dev-utils": "^10.2.1",
        "sass-loader": "8.0.2",
        "style-loader": "0.23.1",
        "terser-webpack-plugin": "2.3.5",
        "ts-node-dev": "^1.0.0-pre.56",
        "ts-pnp": "1.1.6",
        "typescript": "^3.9.7",
        "url-loader": "2.3.0",
        "webpack": "4.42.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-manifest-plugin": "2.2.0",
        "workbox-webpack-plugin": "4.3.1"
    },
    "proxy": "http://localhost:5000"
}
